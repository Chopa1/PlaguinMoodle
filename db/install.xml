<XMLDB PATH="blocks/calculator/db" VERSION="20220224" COMMENT="XMLDB file for Moodle blocks/calculator">
    <TABLES>
        <TABLE NAME="calculator_history" COMMENT="Table for calculator history">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" COMMENT="Primary key"/>
                <FIELD NAME="a" TYPE="float" LENGTH="10" NOTNULL="true" COMMENT="Value a" SEQUENCE="false"/>
                <FIELD NAME="b" TYPE="float" LENGTH="10" NOTNULL="true" COMMENT="Value b" SEQUENCE="false"/>
                <FIELD NAME="c" TYPE="float" LENGTH="10" NOTNULL="true" COMMENT="Value c" SEQUENCE="false"/>
                <FIELD NAME="x1" TYPE="float" LENGTH="10" NOTNULL="true" COMMENT="Solution x1" SEQUENCE="false"/>
                <FIELD NAME="x2" TYPE="float" LENGTH="10" NOTNULL="true" COMMENT="Solution x2" SEQUENCE="false"/>
                <FIELD NAME="timestamp" TYPE="datetime" LENGTH="10" NOTNULL="true" DEFAULT="CURRENT_TIMESTAMP" COMMENT="Timestamp" SEQUENCE="false"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>